{
  "expo": {
    "name": "SafetyList",
    "slug": "safety-list-app",
    "scheme": "safetylist-app",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#be1622"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "backgroundColor": "#be1622",
      "supportsTablet": true,
      "bundleIdentifier": "com.safety2u.safetylist",
      "buildNumber": "2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#be1622"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.safety2u.safetylist",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir acesso à câmera no $(PRODUCT_NAME)?"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "46c5d89b-601d-440f-99bc-d6f66983ceab"
      }
    },
    "owner": "victorfer18"
  }
}